<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.yucheng.whattowear.Linkapp">
    <TextView
        android:id="@+id/textView"
        android:layout_width="fill_parent"
        android:text="Setting"
        android:layout_height="40dp"
        android:textColor="@color/colorAccent"
        android:textSize="30dp">
    </TextView>
    <TextView
        android:id="@+id/calendar"
        android:layout_width="200dp"
        android:text="Google Calendar"
        android:layout_height="30dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20dp"
        android:layout_below="@id/textView">
    </TextView>

    <TextView
        android:id="@+id/creason"
        android:layout_width="fill_parent"
        android:text="生日,活動(尾牙,宴會,遠遊),星座,開會日期"
        android:layout_height="20dp"
        android:textColor="@color/bright_foreground_material_light"
        android:layout_below="@+id/calendar"
        android:textSize="10dp">
    </TextView>
    <TextView
        android:id="@+id/map"
        android:layout_width="200dp"
        android:text="Google Map"
        android:layout_height="30dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20dp"
        android:layout_below="@id/creason">
    </TextView>

    <TextView
        android:id="@+id/mreason"
        android:layout_width="fill_parent"
        android:text="國家,縣市"
        android:layout_height="20dp"
        android:textColor="@color/bright_foreground_material_light"
        android:layout_below="@+id/map"
        android:textSize="10dp">
    </TextView>
    <TextView
        android:id="@+id/clock"
        android:layout_width="200dp"
        android:text="Clock"
        android:layout_height="30dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20dp"
        android:layout_below="@id/mreason">
    </TextView>

    <TextView
        android:id="@+id/ccreason"
        android:layout_width="fill_parent"
        android:text="上班時間(早班,夜班,大夜班)"
        android:layout_height="20dp"
        android:textColor="@color/bright_foreground_material_light"
        android:layout_below="@+id/clock"
        android:textSize="10dp">
    </TextView>
    <Button
        android:id="@+id/buttonObj3"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="OK"
        android:layout_centerVertical="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="admit"
        android:id="@+id/checkBox5"
        android:layout_alignTop="@+id/calendar"
        android:layout_toEndOf="@+id/calendar" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="admit"
        android:id="@+id/checkBox6"
        android:layout_below="@+id/creason"
        android:layout_toEndOf="@+id/map" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="admit"
        android:id="@+id/checkBox7"
        android:layout_below="@+id/mreason"
        android:layout_toEndOf="@+id/clock" />
</RelativeLayout>
